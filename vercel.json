{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      },
      {
        "src": "backend/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "backend/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  